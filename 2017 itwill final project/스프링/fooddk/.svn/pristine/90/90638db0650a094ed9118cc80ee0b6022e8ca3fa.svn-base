package fooddk.domain;

public class Grade {
	private int g_no;
	private String g_grade;
	
	public Grade() {
		// TODO Auto-generated constructor stub
	}	
	
	public Grade(String g_grade) {
		// TODO Auto-generated constructor stub
		this.g_grade = g_grade;
	}	
	
	public Grade(int g_no, String g_grade) {
		super();
		this.g_no = g_no;
		this.g_grade = g_grade;
	}
	public int getG_no() {
		return g_no;
	}
	public void setG_no(int g_no) {
		this.g_no = g_no;
	}
	public String getG_grade() {
		return g_grade;
	}
	public void setG_grade(String g_grade) {
		this.g_grade = g_grade;
	}
	@Override
	public String toString() {
		return "Grade [g_no=" + g_no + ", g_grade=" + g_grade + "]";
	}
	
	
}
